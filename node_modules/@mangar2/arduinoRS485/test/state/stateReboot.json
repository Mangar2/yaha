[
    {
        "description": "State Reboot",
        "tests": [
            {
                "description": "Goto Reboot",
                "noMessage": 101,
                "result": "stateChanged"
            },
            {
                "description": "Keep on RegistrationInfo",
                "request": "registrationInfo",
                "expected": { "result": "stateUnchanged", "maySend": false }
            },
            {
                "description": "Inform about Registration on RegistrationInfo",
                "request": "registrationRequest",
                "expected": { "result": "registrationInfo", "maySend": false }
            }
        ]
    },
    {
        "description": "State Reboot",
        "tests": [
            {
                "description": "Goto Reboot",
                "noMessage": 101,
                "result": "stateChanged"
            },
            {
                "description": "Change on enable send",
                "request": "enableSend",
                "expected": { "result": "stateChanged", "maySend": true }
            }
        ]
    },
    {
        "description": "State Reboot",
        "tests": [
            {
                "description": "Goto Reboot",
                "noMessage": 101,
                "result": "stateChanged"
            },
            {
                "description": "noTimeout first",
                "noMessage": 1,
                "expected": {
                    "result": "enableSend",
                    "neighbour": "unknown",
                    "leftmostCeibling": "unknown"
                }
            },
            {
                "description": "noTimeout rest",
                "noMessage": 99,
                "expected": { "result": "stateUnchanged", "maySend": false }
            },
            {
                "description": "timeout",
                "noMessage": 1,
                "expected": { "result": "stateChanged", "maySend": false }
            }
        ]
    }
]