[
    {
        "description": "State Registered",
        "tests": [
            {
                "description": "Goto Unregistered",
                "request": "registrationRequest"
            },
            {
                "description": "Goto Registered",
                "request": "enableSend"
            },
            {
                "description": "Keep on RegistrationInfo",
                "request": "registrationInfo",
                "expected": { "result": "stateUnchanged", "maySend": true  }
            },
            {
                "description": "Inform about Registration",
                "request": "registrationRequest",
                "expected": { "result": "stateUnchanged", "maySend": true }
            }
        ]
    },
    {
        "description": "State Registered",
        "tests": [
            {
                "description": "Goto Unregistered",
                "request": "registrationRequest"
            },
            {
                "description": "Goto Registered",
                "request": "enableSend"
            },
            {
                "description": "Change on enable send",
                "request": "enableSend",
                "expected": { "result": "stateUnchanged", "maySend": true }
            }
        ]
    },
    {
        "description": "State Registered",
        "tests": [
            {
                "description": "Goto Unregistered",
                "request": "registrationRequest"
            },
            {
                "description": "Goto Registered",
                "request": "enableSend"
            },
            {
                "description": "noTimeout first",
                "noMessage": 1,
                "expected": { "result": "stateUnchanged", "maySend": true }
            },
            {
                "description": "Short Break",
                "noMessage": 3,
                "expected": { "result": "registrationRequest", "maySend": false }
            },
            {
                "description": "Long Break",
                "noMessage": 4,
                "expected": { "result": "enableSend", "maySend": false }
            },
            {
                "description": "noTimeout rest",
                "noMessage": 92,
                "expected": { "result": "stateUnchanged", "maySend": false }
            },
            {
                "description": "timeout",
                "noMessage": 1,
                "expected": { "result": "stateChanged", "maySend": false }
            }
        ]
    }
]