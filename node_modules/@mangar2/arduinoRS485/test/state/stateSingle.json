[
    {
        "description": "State Single",
        "tests": [
            {
                "description": "Goto Single",
                "noMessage": 202,
                "result": "stateChanged"
            },
            {
                "description": "Keep on RegistrationInfo",
                "request": "registrationInfo",
                "expected": { "result": "stateChanged" }
            },
            {
                "description": "Inform about Registration",
                "request": "registrationRequest",
                "expected": { "result": "registrationInfo", "maySend": false }
            },
            {
                "description": "Back to ungegistered",
                "request": "registrationInfo",
                "expected": { "result": "stateUnchanged" }
            }
        ]
    },
    {
        "description": "State Single",
        "tests": [
            {
                "description": "Goto Single",
                "noMessage": 202,
                "result": "stateChanged"
            },
            {
                "description": "Change on enable send",
                "request": "enableSend",
                "expected": { "result": "stateChanged", "maySend": true }
            }
        ]
    },
    {
        "description": "State Single",
        "tests": [
            {
                "description": "Goto Single",
                "noMessage": 202,
                "result": "stateChanged"
            },
            {
                "description": "noTimeout first",
                "noMessage": 1,
                "expected": {
                    "maySend": false,
                    "result": "registrationRequest"
                }
            },
            {
                "description": "Short Break",
                "noMessage": 3,
                "expected": { "result": "stateUnchanged", "maySend": true }
            },
            {
                "description": "Long Break",
                "noMessage": 4,
                "expected": { "result": "stateUnchanged", "maySend": true }
            },
            {
                "description": "noTimeout rest",
                "noMessage": 92,
                "expected": { "result": "stateUnchanged", "maySend": true }
            },
            {
                "description": "timeout",
                "noMessage": 1,
                "expected": { "result": "stateUnchanged", "maySend": true }
            }
        ]
    }
]