[
    {
        "description": "test and/or",
        "variables":{
            "a/b": 1,
            "b/c": 2,
            "system/presence": "awake"
        },
        "tests": [
            {
                "description": "unary and evaluates to element",
                "tree": ["and", ["=", "a/b", 1]],
                "result": true,
                "reason": "a/b (1) is equal to 1"
            },
            {
                "description": "unary and evaluates to element",
                "tree": ["and", false],
                "result" : false,
                "reason": "constant false"
            },
            {
                "description": "unary or evaluates to element",
                "tree": ["or", ["=", "a/b", 1]],
                "result" : true
            },
            {
                "description": "unary or evaluates to element",
                "tree": ["or", false],
                "result" : false,
                "reason": "constant false"
            },
            {
                "description": "and all true",
                "tree": ["and", true, true, true, ["=", "a/b", 1], true],
                "result": true,
                "reason": "constant true and constant true and constant true and a/b (1) is equal to 1 and constant true"
            },
            {
                "description": "and one false",
                "tree": ["and", true, true, true, [">", "a/b", 1], true],
                "result": false
            },
            {
                "description": "or all false",
                "tree": ["or", false, false, false, ["<", "a/b", 1], false],
                "result": false
            },
            {
                "description": "or one true",
                "tree": ["or", false, false, false, ["=", "a/b", 1], false],
                "result": true,
                "reson": "a/b (1) is equal to 1"
            }

        ]
    }
]