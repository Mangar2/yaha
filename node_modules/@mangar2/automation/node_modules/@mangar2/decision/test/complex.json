[{
    "description": "test clomplex terms",
    "variables":{
        "a/b": 1,
        "b/c": 2,
        "system/presence": "awake",
        "/time": "10:0"
    },
    "tests": [
        {
            "description": "and to true",
            "tree": [
                "and",
                ["below", "a/b", 16],
                ["above", "b/c", ["switch", "system/presence", { "absent": 2, "awake": 1, "sleeping": 3 }]]
            ],
            "result": true,
            "reason": "a/b (1) is below 16 and b/c (2) is above system/presence (awake) mapped to 1"
        },
        {
            "description": "and to false",
            "tree": [
                "and",
                ["below", "a/b", 16],
                ["above", "b/c", ["switch", "system/presence", { "absent": 1, "awake": 2, "sleeping": 3 }]]
            ],
            "result": false,
            "reason": "b/c (2) is not above system/presence (awake) mapped to 2"
        },
        {
            "description": "or to true",
            "tree": [
                "and",
                ["or", ["below", "a/b", 10], ["below", "b/c", 2]],
                ["or", ["above or equal", "a/b", 10], ["above or equal", "b/c", 2]]
            ],
            "result": true,
            "reason": "a/b (1) is below 10 and b/c (2) is above or equal 2"
        },
        {
            "description": "undefined variable",
            "tree": [
                "and",
                ["or", ["below", "a/b", 10], ["below", "a/b/c", 2]],
                ["or", ["above or equal", "a/b", 10], ["above or equal", "b/c", 2]]
            ],
            "result": false,
            "reason": "false, undefined variables: a/b/c"
        },
        {
            "description": "not enough operands",
            "tree": ["above", "a/b"],
            "result": false,
            "reason": "term array needs three elements[\"above\",\"a/b\"]"
        },
        {
            "description": "unknown operator",
            "tree": ["unknown", "a/b", 10],
            "result": false,
            "reason": "unknown operator unknown"
        }
    ]
}]