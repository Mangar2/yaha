{
    "variables":{
        "a/b": 1,
        "b/c": 2,
        "system/presence": "awake"
    },
    "tests": [
        {
            "name": "unary and evaluates to element",
            "tree": ["and", ["=", "a/b", 1]],
            "result": true,
            "reason": "a/b (1) is equal to 1"
        },
        {
            "name": "unary and evaluates to element",
            "tree": ["and", false],
            "result" : false,
            "reason": "constant false"
        },
        {
            "name": "unary or evaluates to element",
            "tree": ["or", ["=", "a/b", 1]],
            "result" : true
        },
        {
            "name": "unary or evaluates to element",
            "tree": ["or", false],
            "result" : false,
            "reason": "constant false"
        },
        {
            "name": "and all true",
            "tree": ["and", true, true, true, ["=", "a/b", 1], true],
            "result": true,
            "reason": "constant true and constant true and constant true and a/b (1) is equal to 1 and constant true"
        },
        {
            "name": "and one false",
            "tree": ["and", true, true, true, [">", "a/b", 1], true],
            "result": false
        },
        {
            "name": "or all false",
            "tree": ["or", false, false, false, ["<", "a/b", 1], false],
            "result": false
        },
        {
            "name": "or one true",
            "tree": ["or", false, false, false, ["=", "a/b", 1], false],
            "result": true,
            "reson": "a/b (1) is equal to 1"
        }

    ]
}