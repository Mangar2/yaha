[{
    "description": "test +/-",
    "variables":{
        "a/b": 1,
        "b/c": 2,
        "system/presence": "awake",
        "/time": "10:0"
    },
    "tests": [
        {
            "description": "minus",
            "tree": ["-", 5, 3],
            "result": 2,
            "reason": "2 (5 - 3)"
        },
        {
            "description": "plus",
            "tree": ["+", 5, 3],
            "result": 8,
            "reason": "8 (5 + 3)"
        },
        {
            "description": "undefined variable",
            "tree": ["+", "/undefined", 10],
            "result": false,
            "reason": "false is not a number or a date ([\"/undefined\",10])"
        },
        {
            "description": "Subtract from time",
            "tree": ["-", "/time", 10],
            "result": "09:50:00",
            "reason": "09:50:00 (/time (10:00:00) - 10)"
        },
        {
            "description": "Add to time",
            "tree": ["+", "/time", 10],
            "result": "10:10:00",
            "reason": "10:10:00 (/time (10:00:00) + 10)"
        }

    ]
}]