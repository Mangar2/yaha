[{
    "description": "test strings",
    "variables":{
        "/time": "08:00",
        "test/mytime": "9:00"
    },
    "tests": [
        {
            "description": "normal",
            "tree": "hello",
            "result": "hello",
            "reason": "hello"
        },
        {
            "description": "number",
            "tree": "8",
            "result": "8",
            "reason": "8"
        },
        {
            "description": "time of day",
            "tree": ["=", "08:00", "/time"],
            "result": true,
            "reason": "08:00 is equal to /time (08:00:00)"
        },
        {
            "description": "time in variable",
            "tree": [">", "test/mytime", "/time"],
            "result": true,
            "reason": "test/mytime (09:00:00) is above /time (08:00:00)"
        }
    ]
}]