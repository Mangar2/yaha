[{
    "description": "test switch",
    "variables":{
        "a/b": 1,
        "b/c": 2,
        "system/presence": "awake",
        "/time": "10:0"
    },
    "tests": [
        {
            "description": "Maps to first",
            "tree": ["switch", "a/b", { "1": "a", "2": "b", "default": "c" }],
            "result": "a",
            "reason": "a/b (1) mapped to a"
        },
        {
            "description": "Maps to second",
            "tree": ["switch", "a/b", { "2": "a", "1": "b", "default": "c" }],
            "result": "b",
            "reason": "a/b (1) mapped to b"
        },
        {
            "description": "Maps to default",
            "tree": ["switch", "a/b", { "3": "a", "4": "b", "default": "c" }],
            "result": "c",
            "reason": "a/b (1) mapped to c"
        },
        {
            "description": "Maps to true",
            "tree": ["switch", ["=", "a/b", 1], { "true": "on", "false": "off" }],
            "result": "on",
            "reason": "a/b (1) is equal to 1 mapped to on"
        }
    ]
}]