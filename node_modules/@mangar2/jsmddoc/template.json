{
    "templates": [
        { "$ref": "document headline" },
        { "$ref": "document contents" },
        { "$ref": "document meta" },
        { "$ref": "global functions" },
        { "$ref": "class list" }
    ],
    "$def": {
        "document headline": [
            { "text": "<!-- This file is generated by jsmddoc version 0.1 -->\n\n" },
            { "text": "# Abstract\n" },
            { 
                "iterate on": "file",
                "for each": {
                    "if exists": "overview",
                    "text": "\n@overview\n"
                }
            },
            { 
                "iterate on": "file",
                "for each": {
                    "if exists": "example",
                    "text": "\nExample\n\n```shell\n@example\n```\n"
                }
            }
        ],
        "document contents": [
            { "text": "\n## Contents\n\n" },
            { "text": "- [Meta](#Meta)\n" },
            { "if exists": "function", "text": "- [Global Functions](#Global-functions)\n" },
            {
                "iterate on": "function",
                "for each": [
                    { "text": "  - [@function](#@function)\n" }
                ]
            },
            {
                "iterate on": "class",
                "for each": [
                    { "text": "- [Class: @class](#Class:-@class)\n" },
                    { "if exists": "param", "text": "  - [Parameters](#@class-Parameters)\n" },
                    { "if exists": "member", "text": "  - [Members](#@class-Members)\n"},
                    { "if exists": "method", "text": "  - [Methods](#@class-Methods)\n"},
                    {
                        "iterate on": "method",
                        "for each": [
                            { "text": "    - [@method](#@method)\n"}
                        ]
                    }

                ]
            }
        ],
        "document meta": [
            { "text": "\n## Meta\n" },
            { 
                "iterate on": "file",
                "for each": {
                    "if exists": "overview",
                    "$ref": "file meta"
                }
            }
        ],
        "file meta": [
            { "text": "\n| | |\n"},
            { "text": "| --- | --- |\n"},
            { "text": "| **File** | @filename |\n" },
            { "text": "| **Abstract** | @overview |\n" },
            { "if exists": "author", "text": "| **Author** | @author |\n" },
            { "if exists": "copyright", "text": "| **Copyright** | @copyright |\n" },
            { "if exists": "license", "text": "| **License** | @license |\n" }
        ],
        "global functions": { "if exists": "function", "$ref": "function list" },
        "class list": [
            {
                "iterate on": "class",
                "for each": [
                    { "$ref": "class headline" },
                    { "if exists": "example", "text": "\n### Example\n\n```javascript\n@example\n```\n"},
                    { "if exists": "param", "text": "\n### @class Parameters\n\n", "$ref": "parameter table" },
                    { "if exists": "member", "text": "\n### @class Members\n\n", "$ref": "member list" },
                    { "if exists": "method", "text": "\n### @class Methods\n", "$ref": "method list" }
                ]
            }
        ],
        "class headline": [
            { "text": "\n## Class: @class\n\n" },
            { "text": "`new @class(" },
            { "$ref": "parameter list" },
            { "text": ")`\n\n" },
            { "if exists": "description", "text": "@description\n" }
        ],
        "parameter table": [
            {
                "iterate on": "param",
                "for each": [ 
                    { "if isfirst": true, "text": "| Name | Type |" },
                    { "if isfirst": true, "if exists": "attributes", "text": " Attribute |" },
                    { "if isfirst": true, "if exists": "default", "text": " Default |" },
                    { "if isfirst": true, "if exists": "description", "text": " Description |" },
                    { "if isfirst": true, "text": "\n" },
                    { "if isfirst": true, "text": "| ---- | ---- | ---- | ---- | ----------- |\n" },
                    { "text": "|" },
                    { "if exists": "name", "text": " `@name` |" },
                    { "if exists": "type", "text": " `@type` |" },
                    { "if exists": "attributes", "text": " @attributes |" },
                    { "if exists": "default", "text": " @default |" },
                    { "if exists": "description", "text": " @description |" },
                    { "text": " |\n" }
                ]
            },
            {
                "iterate on": "param",
                "for each": {
                    "if exists": "param",
                    "text": "\n#### @name properties\n\n",
                    "$ref": "parameter table"
                }
            }
        ],
        "parameter list": {
            "iterate on": "param",
            "for each": { "text": "@name", "separator": ", " }
        },
        "return table": [
            { "if exists": "method", "text": "\n##### @method returns\n\n" },
            { "if exists": "function", "text": "\n#### @function returns\n\n" },
            { "text": "| Type | Description |\n| ---- | ----------- |\n" },
            {
                "iterate on": "returns",
                "for each": { "text": "| `@type` | @description |\n" }
            }
        ],
        "throws table": [
            { "if exists": "method", "text": "\n##### @method throws\n\n" },
            { "if exists": "function", "text": "\n#### @function throws\n\n" },
            { "text": "| Type | Description |\n| ---- | ----------- |\n" },
            {
                "iterate on": "throws",
                "for each": { "text": "| `@type` | @description |\n" }
            }
        ],
        "method": [
            { "text": "\n#### @method\n\n" },
            { "text": "`" },
            { "if exists": "attributes", "text": "@attributes " },
            { "text": "@method (" },
            { "$ref": "parameter list" },
            { "text": ")" },
            { "iterate on": "returns", "for each": { "text": " => {@type}"} },
            { "text": "`\n\n" },
            { "if exists": "description", "text": "@description\n" }
        ],
        "method list": {
            "iterate on": "method",
            "for each": [
                { "$ref": "method" },
                { "if exists": "param", "text": "\n##### @method Parameters\n\n", "$ref": "parameter table" },
                { "if exists": "throws", "$ref": "throws table" },
                { "if exists": "returns", "$ref": "return table" }
            ]
        },
        "function": [
            { "text": "\n### @function\n\n" },
            { "text": "`" },
            { "if exists": "attributes", "text": "@attributes " },
            { "text": "@function (" },
            { "$ref": "parameter list" },
            { "text": ")" },
            { "iterate on": "returns", "for each": { "text": " => {@type}"} },
            { "text": "`\n\n" },
            { "if exists": "description", "text": "@description\n" },
            { "if exists": "example", "text": "\n#### Example\n\n```javascript\n@example\n```\n"}
        ],
        "function list": {
            "iterate on": "function",
            "for each": [
                { "if isfirst": true, "text": "\n## Global functions\n\nAll global non private functions are listed here\n" },
                { "$ref": "function" },
                { "if exists": "param", "text": "\n#### @function Parameters\n\n", "$ref": "parameter table" },
                { "if exists": "throws", "$ref": "throws table" },
                { "if exists": "returns", "$ref": "return table" }
            ]
        },
        "member list": {
            "iterate on": "member",
            "for each": [
                { "if isfirst": true, "text": "| Name | Type |" },
                { "if isfirst": true, "if exists": "attributes", "text": " attributes |" },
                { "if isfirst": true, "text": " description |\n" },
                { "if isfirst": true, "text": "| ---- | ---- | ---- | ---------- |\n" },
                { "text": "| `@member` | `@type` |" },
                { "if exists": "attributes", "text": " @attributes |" },
                { "text": " @description |\n" }
            ]
        }
    }
}
