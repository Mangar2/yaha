[
    {
        "description": "class",
        "tests": [
            {
                "description": "description without tag",
                "input": "/** description is default */",
                "expected": {
                    "description": "description is default"
                }
            },
            {
                "description": "description with tag",
                "input": "/** @description description is default */",
                "expected": {
                    "description": "description is default"
                }
            },
            {
                "description": "description with tag",
                "input": "/** @description description is default */ class myclass {} ",
                "expected": {
                    "class": [
                        {
                            "description": "description is default",
                            "class": "myclass",
                            "details": []
                        }
                    ]
                }
            }
        ]
    },
    {
        "description": "method",
        "tests": [
            {
                "description": "simple method",
                "input": "/** class description */ class myclass { /** test */ test() {} }",
                "expected": {
                    "class": [
                        {
                            "description": "class description",
                            "class": "myclass",
                            "details": [
                                {
                                    "description": "test",
                                    "method": "test"
                                }
                            ]
                        }
                    ]
                }
            },
            {
                "description": "getter",
                "input": "/** class description */ class myclass { /** my member @type {integer} */ get test () {} }",
                "expected": {
                    "class": [
                        {
                            "description": "class description",
                            "class": "myclass",
                            "details": [
                                {
                                    "description": "test",
                                    "method": "test"
                                }
                            ]
                        }
                    ]
                }
            }
        ]
    }
]