[
    {
        "description": "meta",
        "tests": [
            {
                "description": "any tag",
                "input": "/** @anytag anyinfo */",
                "expected": {
                    "anytag": "anyinfo"
                }
            },
            {
                "description": "licence",
                "input": "/** @license licensetest */",
                "expected": {
                    "license": "licensetest"
                }
            },
            {
                "description": "author",
                "input": "/** @author Max Mustermann */",
                "expected": {
                    "author": "Max Mustermann"
                }
            },
            {
                "description": "copyright",
                "input": "/** @copyright Jane Doe */",
                "expected": {
                    "copyright": "Jane Doe"
                }
            }
        ]
    }
]