[
    {
        "description": "parameter",
        "tests": [
            {
                "description": "only name",
                "input": "/** @param myparam */",
                "expected": {
                    "param": [ { "name": "myparam" } ]
                }
            },
            {
                "description": "use arg instead of param",
                "input": "/** @arg myparam */",
                "expected": {
                    "param": [ { "name": "myparam" } ]
                }
            },
            {
                "description": "have type",
                "input": "/** @param {string} myparam */",
                "expected": {
                    "param": [ { "name": "myparam", "type": "string" } ]
                }
            },
            {
                "description": "two types",
                "input": "/** @param {string|number} myparam */",
                "expected": {
                    "param": [ { "name": "myparam", "type": "string, number" } ]
                }
            },
            {
                "description": "is optional",
                "input": "/** @param [myparam] */",
                "expected": {
                    "param": [ { "name": "myparam", "attributes": "optional" } ]
                }
            },
            {
                "description": "has default value",
                "input": "/** @param [myparam = 1] */",
                "expected": {
                    "param": [ { "name": "myparam", "attributes": "optional", "default": "1" } ]
                }
            },
            {
                "description": "has description",
                "input": "/** @param myparam this is my description */",
                "expected": {
                    "param": [ { "name": "myparam", "description": "this is my description" } ]
                }
            },
            {
                "description": "two parameters",
                "input": "/** @param myparam this is my description @param param2 */",
                "expected": {
                    "param": [ 
                        { "name": "myparam", "description": "this is my description" },
                        { "name": "param2", "description": "" }
                     ]
                }
            },
            {
                "description": "subparameter",
                "input": "/** @param myparam this is my description @param {number} myparam.subparam my subparam @param param2 */",
                "expected": {
                    "param": [ 
                        { 
                            "name": "myparam", 
                            "description": "this is my description", 
                            "param": [
                                { "name": "subparam", "type": "number", "description": "my subparam" }
                            ]
                        },
                        { "name": "param2", "description": "" }
                     ]
                }
            }
        ]

    }
]