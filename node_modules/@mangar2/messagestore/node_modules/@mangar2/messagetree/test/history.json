[
    {
        "description": "two nodes",
        "add": [
            ["", "value1"],
            ["", "value2"]
        ],
        "tests": [
            {
                "description": "newest node",
                "getNode": "",
                "time": "timestamp",
                "expected": {
                    "value": "value2",
                    "history": [
                        {
                            "value": "value1",
                            "type": "single",
                            "time": "timestamp"
                        }
                    ]
                }
            }
        ]
    },
    {
        "description": "three nodes",
        "add": [
            ["", "value1"],
            ["", "value2"],
            ["", "value3"]
        ],
        "tests": [
            {
                "description": "timeValue",
                "getNode": "",
                "expected": {
                    "value": "value3",
                    "history": [{
                        "type": "timeValue",
                        "values": [[ "timestamp", "value1" ], ["timestamp", "value2"]]
                    }]
                }
            }
        ]
    },
    {
        "description": "different reason",
        "add": [
            ["", "value1", "a"],
            ["", "value2", "b"],
            ["", "value3", "c"]
        ],
        "tests": [
            {
                "description": "timeValue",
                "getNode": "",
                "expected": {
                    "value": "value3",
                    "reason": "c",
                    "history": [
                        { "type": "single", "value": "value2", "reason": "b" },
                        { "type": "single", "value": "value1", "reason": "a" }
                    ]
                }
            }
        ]
    }

]