[
    {
        "description": "split time from timeValue",
        "config": {
            "lengthForFurtherCompression": 3,
            "upperBoundAddInMilliseconds": 1,
            "lowerBoundAddInMilliseconds": 1
        },
        "add": [
            ["a", "value1", "same"],
            ["a", "value2", "same"],
            ["a", "value2", "same"],
            ["a", "value2", "same"],
            ["a", "value2", "same"]
        ],
        "delay": [1,1,1,10,1],
        "tests": [
            {
                "description": "node a",
                "getNode": "a",
                "expected": {
                    "value": "value2",
                    "reason": "same",
                    "history": [
                        {
                            "type": "time",
                            "value": "value2",
                            "timestamps": ["timestamp", "timestamp", "timestamp"],
                            "reason": "same"
                        },
                        {
                            "type": "timeValue",
                            "reason": "same",
                            "values": [[ "timestamp", "value1" ]]
                        }
                    ]
                }
            }
        ]
    },
    {
        "description": "split interval from timeValue",
        "config": {
            "lengthForFurtherCompression": 3
        },
        "add": [
            ["a", "value1", "same"],
            ["a", "value2", "same"],
            ["a", "value2", "same"],
            ["a", "value2", "same"],
            ["a", "value2", "same"]
        ],
        "delay": [1,1,1,1,1],
        "tests": [
            {
                "description": "node a",
                "getNode": "a",
                "expected": {
                    "value": "value2",
                    "reason": "same",
                    "history": [
                        {
                            "type": "interval",
                            "value": "value2",
                            "amount": 3,
                            "reason": "same"
                        },
                        {
                            "type": "timeValue",
                            "reason": "same",
                            "values": [[ "timestamp", "value1" ]]
                        }
                    ]
                }
            }
        ]
    }
]