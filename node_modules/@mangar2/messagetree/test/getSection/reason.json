[
    {
        "description": "two values",
        "add": [["", "value1", "reason1"], ["", "value2", "reason2"]],
        "tests": [
            {
                "description": "with history and reason",
                "topic": "",
                "levelAmount": 2,
                "addHistory": true,
                "addReason": true,
                "expected": {
                    "topics": ["/"],
                    "payload": {
                        "/": {
                            "topic": "",
                            "time": "1970-01-01T00:00:00.000Z",
                            "value": "value2",
                            "reason": [
                                {
                                  "message": "reason2",
                                  "timestamp": "1970-01-01T00:00:00.000Z"
                                }
                            ],
                            "history": [
                                {
                                    "time": "1970-01-01T00:00:00.000Z",
                                    "value": "value1",
                                    "reason": [
                                    {
                                       "message": "reason1",
                                        "timestamp": "1970-01-01T00:00:00.000Z"
                                    }]
                                }
                            ]
                        }
                    }
                }
            },
            {
                "description": "without history with reason",
                "topic": "",
                "levelAmount": 2,
                "addHistory": false,
                "addReason": true,
                "expected": {
                    "topics": ["/"],
                    "payload": {
                        "/": {
                            "topic": "",
                            "time": "1970-01-01T00:00:00.000Z",
                            "value": "value2",
                            "reason": [
                                {
                                    "message": "reason2",
                                    "timestamp": "1970-01-01T00:00:00.000Z"
                                }
                            ]
                        }
                    }
                }
            },
            {
                "description": "without history and reason",
                "topic": "",
                "levelAmount": 2,
                "addHistory": false,
                "addReason": false,
                "expected": {
                    "topics": ["/"],
                    "payload": {
                        "/": {
                            "topic": "",
                            "time": "1970-01-01T00:00:00.000Z",
                            "value": "value2"
                        }
                    }
                }
            }
        ]
    }
]