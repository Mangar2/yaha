[
    {
        "description": "further compressed",
        "config": {
            "lengthForFurtherCompression": 2
        },
        "add": [
            ["a", 1, "same"],
            ["a", 1, "same"],
            ["a", 1, "same"],
            ["a", 1, "same"]
        ],
        "tests": [
            {
                "description": "four",
                "getNode": "a",
                "expected": {
                    "value": 1,
                    "reason": "same",
                    "history": [{
                        "type": "interval",
                        "reason": "same",
                        "firstTime": "timestamp",
                        "lastTime": "timestamp",
                        "amount": 3,
                        "value": 1
                    }]
                }
            }
        ]
    },
    {
        "description": "add more",
        "config": {
            "lengthForFurtherCompression": 2,
            "maxValuesPerHistoryEntry": 4
        },
        "add": [
            ["a", 1, "same"],
            ["a", 1, "same"],
            ["a", 1, "same"],
            ["a", 1, "same"],
            ["a", 1, "same"],
            ["a", 1, "same"],
            ["a", 1, "same"]
        ],
        "tests": [
            {
                "description": "ignores maxValues",
                "getNode": "a",
                "expected": {
                    "value": 1,
                    "reason": "same",
                    "history": [{
                        "type": "interval",
                        "reason": "same",
                        "firstTime": "timestamp",
                        "lastTime": "timestamp",
                        "amount": 6,
                        "value": 1
                    }]
                }
            }
        ]
    },
    {
        "description": "some delay",
        "config": {
            "lengthForFurtherCompression": 3,
            "upperBoundAddInMilliseconds": 10,
            "lowerBoundAddInMilliseconds": 10
        },
        "add": [
            ["a", 1, "same"],
            ["a", 1, "same"],
            ["a", 1, "same"],
            ["a", 1, "same"]
        ],
        "delay": [1,15,4,2],
        "tests": [
            {
                "description": "four",
                "getNode": "a",
                "expected": {
                    "history": [{
                        "type": "interval",
                        "firstTime": "timestamp",
                        "lastTime": "timestamp",
                        "amount": 3
                    }]
                }
            }
        ]
    },
    {
        "description": "some delay",
        "config": {
            "lengthForFurtherCompression": 3,
            "upperBoundAddInMilliseconds": 10,
            "lowerBoundAddInMilliseconds": 10
        },
        "add": [
            ["a", 1, "same"],
            ["a", 1, "same"],
            ["a", 1, "same"],
            ["a", 1, "same"],
            ["a", 1, "same"],
            ["a", 1, "same"]
        ],
        "delay": [2,2,2,2, 50, 10],
        "tests": [
            {
                "description": "four",
                "getNode": "a",
                "expected": {
                    "history": [
                        { "value": 1, "type": "single" },
                        {
                            "type": "interval",
                            "firstTime": "timestamp",
                            "lastTime": "timestamp",
                            "amount": 4
                        }
                    ]
                }
            }
        ]
