[
    {
        "description": "noneOf tests",
        "rule": {
            "allOf": ["/required1"],
            "noneOf": ["/event1", "/event2"],
            "topic": "topic",
            "value": "value"
        },
        "tests": [
            {
                "description": "required provided",
                "time": "8:00",
                "motionEvents": ["/required1"],
                "expected": { "messages": [{ "topic": "topic", "value": "value"}] }
            },
            {
                "description": "all noneof provided",
                "time": "8:01",
                "motionEvents": ["/required1", "/event1", "/event2"],
                "expected": { "messages": [] }
            },
            {
                "description": "one none of provided",
                "time": "8:02",
                "motionEvents": ["/required1", "/event1"],
                "expected": { "messages": [] }
            },
            {
                "description": "one additional provided",
                "time": "8:03",
                "motionEvents": ["/required1", "/event3"],
                "expected": { "messages": [{ "topic": "topic", "value": "value"}] }
            }
        ]
    }
]
