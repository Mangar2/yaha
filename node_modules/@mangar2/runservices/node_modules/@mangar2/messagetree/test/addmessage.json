[
    {
        "description": "RootNode",
        "add": [
            ["", "aValue", "aReason"]
        ],
        "tests": [
            {
                "description": "root message",

                "getNode": "",
                "expected": {
                    "value": "aValue",
                    "reason": "aReason"
                }
            }
        ]
    },
    {
        "description": "different nodes on level 1",
        "add": [
            ["a", "va", "ra"],
            ["b", "vb", "rb"],
            ["c", "vc", "rc"]
        ],
        "tests": [
            {
                "description": "branch a",
                "getNode": "a",
                "expected": {
                    "value": "va",
                    "reason": "ra"
                }
            },
            {
                "description": "branch b",
                "getNode": "b",
                "expected": {
                    "value": "vb",
                    "reason": "rb"
                }
            },
            {
                "description": "branch c",
                "getNode": "c",
                "expected": {
                    "value": "vc",
                    "reason": "rc"
                }
            }
        ]
    },
    {
        "description": "different nodes on different levels",
        "add": [
            ["a", "va", "ra"],
            ["a/b", "vb", "rb"],
            ["a/b/c", "vc", "rc"]
        ],
        "tests": [
            {
                "description": "branch a",
                "getNode": "a",
                "expected": {
                    "value": "va",
                    "reason": "ra"
                }
            },
            {
                "description": "branch b",
                "getNode": "a/b",
                "expected": {
                    "value": "vb",
                    "reason": "rb"
                }
            },
            {
                "description": "branch c",
                "getNode": "a/b/c",
                "expected": {
                    "value": "vc",
                    "reason": "rc"
                }
            }
        ]
    },
    {
        "description": "mix",
        "add": [
            ["a/b/c", "va", "ra"],
            ["b/a/c", "vb", "rb"],
            ["b/c/a", "vc", "rc"]
        ],
        "tests": [
            {
                "description": "branch a",
                "getNode": "a/b/c",
                "expected": {
                    "value": "va",
                    "reason": "ra"
                }
            },
            {
                "description": "branch b",
                "getNode": "b/a/c",
                "expected": {
                    "value": "vb",
                    "reason": "rb"
                }
            },
            {
                "description": "branch c",
                "getNode": "b/c/a",
                "expected": {
                    "value": "vc",
                    "reason": "rc"
                }
            }
        ]
    }
]
